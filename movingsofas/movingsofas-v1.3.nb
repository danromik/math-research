        SubscriptBox["\[Kappa]", 
         RowBox[{"6", ",", "1"}]], ",", 
        SubscriptBox["\[Kappa]", 
         RowBox[{"6", ",", "2"}]], ",", 
        SubscriptBox["\[Kappa]", 
         RowBox[{"5", ",", "1"}]], ",", 
        SubscriptBox["\[Kappa]", 
         RowBox[{"5", ",", "2"}]], ",", 
        SubscriptBox["a", "1"], ",", 
        SubscriptBox["a", "2"], ",", 
        SubscriptBox["f", "1"], ",", 
        SubscriptBox["f", "2"], ",", 
        SubscriptBox["e", "1"], ",", 
        SubscriptBox["e", "2"]}], "}"}]}], "\[IndentingNewLine]", "]"}], "[", 
    
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}]], "Input",
