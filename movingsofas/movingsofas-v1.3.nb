Cell[TextData[{
 "Success! We\[CloseCurlyQuote]ve found that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "=", 
    RowBox[{
     RowBox[{"Cos", "[", 
      FractionBox["\[Beta]", "2"], "]"}], "=", 
     RowBox[{
      FractionBox["1", "2"], " ", 
      SqrtBox[
       RowBox[{"2", "-", 
        SqrtBox["2"], "+", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"2", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "4"}], "+", 
             RowBox[{"3", " ", 
