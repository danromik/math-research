Cell["New Bernoulli number summation identities", "Subsection",
 CellChangeTimes->{{3.635010212701461*^9, 3.635010217825817*^9}}],

Cell[TextData[{
 StyleBox["This subsection numerically verifies for small values of ",
  FontSize->16],
 StyleBox["n",
  FontSize->16,
  FontSlant->"Italic"],
 StyleBox[" the two new Bernoulli number summation identities that appear in \
Section 12.1 of the paper.\n\nFirst, we define the functions P[n,k] and \
Q[n,k], and the expressions on the left-hand side and right-hand side of \
equations (107) and (108).",
  FontSize->16]
}], "Text",
 CellChangeTimes->{{3.635010220369752*^9, 3.635010253144505*^9}, {
  3.6350103430067*^9, 3.635010350181772*^9}, {3.635010680290547*^9, 
  3.635010699184507*^9}, {3.6843760223420362`*^9, 3.68437602477918*^9}, {
  3.684600401069148*^9, 3.684600403306315*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"P", "[", 
    RowBox[{"n_", ",", "k_"}], "]"}], ":=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "n"}], "-", "1"}], ")"}], "2"], "-", 
    RowBox[{"4", 
     RowBox[{"(", 
      RowBox[{"k", "-", "1"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"n", "-", "k"}], ")"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Q", "[", 
     RowBox[{"n_", ",", "k_"}], "]"}], ":=", 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["k", "2"], "-", 
         RowBox[{"k", " ", "n"}], "+", 
         SuperscriptBox["n", "2"]}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "1"}], "+", 
         RowBox[{"4", " ", 
          SuperscriptBox["k", "2"]}], "-", 
         RowBox[{"4", " ", "k", " ", "n"}], "+", 
         RowBox[{"4", " ", 
          SuperscriptBox["n", "2"]}]}], ")"}]}], "-", 
      RowBox[{"6", " ", "k", " ", 
       RowBox[{"(", 
        RowBox[{"k", "-", "n"}], ")"}], " ", "n"}]}], 
     RowBox[{"(", 
      RowBox[{"n", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "k"}], "+", "1"}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "n"}], "-", 
         RowBox[{"2", "k"}], "+", "1"}], ")"}]}], ")"}]]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LHS107", "=", 
   FractionBox[
    RowBox[{"BernoulliB", "[", 
     RowBox[{
      RowBox[{"6", "n"}], "-", "2"}], "]"}], 
    RowBox[{
     RowBox[{"6", "n"}], "-", "2"}]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"RHS107", "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["1", 
       RowBox[{"2", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"6", "n"}], "-", "1"}], ")"}]}]]}], 
     RowBox[{"Binomial", "[", 
      RowBox[{
       RowBox[{"4", "n"}], ",", 
       RowBox[{"2", "n"}]}], "]"}], 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Binomial", "[", 
         RowBox[{
          RowBox[{"2", "n"}], ",", 
          RowBox[{
           RowBox[{"2", "k"}], "-", "1"}]}], "]"}], 
        RowBox[{"P", "[", 
         RowBox[{"n", ",", "k"}], "]"}], 
        FractionBox[
         RowBox[{"BernoulliB", "[", 
          RowBox[{
           RowBox[{"2", "n"}], "+", 
           RowBox[{"2", "k"}], "-", "2"}], "]"}], 
         RowBox[{
          RowBox[{"2", "n"}], "+", 
          RowBox[{"2", "k"}], "-", "2"}]], 
        FractionBox[
         RowBox[{"BernoulliB", "[", 
          RowBox[{
           RowBox[{"4", "n"}], "-", 
           RowBox[{"2", "k"}]}], "]"}], 
         RowBox[{
          RowBox[{"4", "n"}], "-", 
          RowBox[{"2", "k"}]}]]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LHS108", "=", 
   FractionBox[
    RowBox[{"BernoulliB", "[", 
     RowBox[{"6", "n"}], "]"}], 
    RowBox[{"6", "n"}]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"RHS108", "=", 
   RowBox[{
    RowBox[{"-", 
     FractionBox["2", 
      RowBox[{"3", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"6", "n"}], "+", "1"}], ")"}]}]]}], 
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"4", "n"}], "+", "1"}], ")"}], "!"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"2", "n"}], ")"}], "!"}], ")"}], "^", "2"}]], 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Binomial", "[", 
        RowBox[{
         RowBox[{"2", "n"}], ",", 
         RowBox[{"2", "k"}]}], "]"}], 
       RowBox[{"Q", "[", 
        RowBox[{"n", ",", "k"}], "]"}], 
       FractionBox[
        RowBox[{"BernoulliB", "[", 
         RowBox[{
          RowBox[{"2", "n"}], "+", 
          RowBox[{"2", "k"}]}], "]"}], 
        RowBox[{
         RowBox[{"2", "n"}], "+", 
         RowBox[{"2", "k"}]}]], 
       FractionBox[
        RowBox[{"BernoulliB", "[", 
         RowBox[{
          RowBox[{"4", "n"}], "-", 
          RowBox[{"2", "k"}]}], "]"}], 
        RowBox[{
         RowBox[{"4", "n"}], "-", 
         RowBox[{"2", "k"}]}]]}], ",", 
      RowBox[{"{", 
       RowBox[{"k", ",", "0", ",", "n"}], "}"}]}], "]"}]}]}], ";"}]}], "Input",\

