     RowBox[{"Zeta", "[", 
      FractionBox["1", "2"], "]"}], 
     RowBox[{"Zeta", "[", 
      FractionBox["3", "2"], "]"}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   SubscriptBox["A", "1"], "=", 
   RowBox[{"5", 
    SuperscriptBox["X", "2"]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   SubscriptBox["A", "2"], "=", 
   RowBox[{
    SuperscriptBox["X", 
     RowBox[{"-", "1"}]], "Y"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   SubscriptBox["A", "3"], "=", 
   RowBox[{
    FractionBox["3", "80"], 
    SuperscriptBox["X", 
     RowBox[{"-", "4"}]], 
    SuperscriptBox["Y", "2"]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    SubscriptBox["A", "4"], "=", 
