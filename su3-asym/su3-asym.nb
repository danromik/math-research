   "A", " ", "much", " ", "simplified", " ", "formula", " ", "proved", " ", 
    "in", " ", "a", " ", "2016", " ", "paper", " ", "by", " ", 
    RowBox[{"J", ".", " ", "Borwein"}], " ", "and", " ", 
