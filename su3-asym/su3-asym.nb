Cell["5. Numerical computation of the sequence r(n)", "Section",
 CellChangeTimes->{{3.635216442217354*^9, 3.6352164539665213`*^9}}],

Cell[TextData[StyleBox["In this section we compute the initial values of the \
sequence r(n) defined section 1.1 of the paper. This is done by expanding the \
generating function given in equation (2) as a power series. Run the code \
below to define the relevant functions.",
 FontSize->16]], "Text",
 CellChangeTimes->{{3.635216469541889*^9, 3.6352165887176447`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"dimtable", "[", "a_", "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"1", "/", "2"}], "m", " ", "n", 
      RowBox[{"(", 
       RowBox[{"m", "+", "n"}], ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "1", ",", "a"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"n", ",", "1", ",", "a"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"irrepdims", "[", "bound_", "]"}], ":=", 
   RowBox[{"Sort", "[", 
    RowBox[{"Select", "[", 
     RowBox[{
      RowBox[{"Flatten", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"1", "/", "2"}], "m", " ", "n", " ", 
          RowBox[{"(", 
           RowBox[{"m", "+", "n"}], ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"m", ",", "1", ",", 
           RowBox[{"Ceiling", "[", 
            RowBox[{"Sqrt", "[", 
             RowBox[{"2", "bound"}], "]"}], "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"n", ",", "1", ",", 
           RowBox[{"Ceiling", "[", 
            RowBox[{"Sqrt", "[", 
             RowBox[{"2", "bound"}], "]"}], "]"}]}], "}"}]}], "]"}], "]"}], 
      ",", 
      RowBox[{
       RowBox[{"#", "\[LessEqual]", "bound"}], "&"}]}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"rvalues", "[", "n_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "dims", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"dims", "=", 
       RowBox[{"irrepdims", "[", "n", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"CoefficientList", "[", 
       RowBox[{
        RowBox[{"Series", "[", 
         RowBox[{
          RowBox[{"Product", "[", 
           RowBox[{
            RowBox[{"1", "/", 
             RowBox[{"(", 
              RowBox[{"1", "-", 
               RowBox[{"x", "^", 
                RowBox[{"dims", "[", 
                 RowBox[{"[", "j", "]"}], "]"}]}]}], ")"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"j", ",", "1", ",", 
              RowBox[{"Length", "[", "dims", "]"}]}], "}"}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "0", ",", "n"}], "}"}]}], "]"}], ",", "x"}], 
       "]"}]}]}], "\[IndentingNewLine]", "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.6352163636818323`*^9, 3.635216377693831*^9}, {
