      RowBox[{"-", "7"}]], 
     SuperscriptBox["Y", "3"]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"IntriguingIntegral", "=", 
   RowBox[{"NIntegrate", "[", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], "\[Times]", 
      FractionBox[
       RowBox[{
        RowBox[{"Zeta", "[", 
         RowBox[{
          FractionBox["3", "2"], "+", 
          RowBox[{"\[ImaginaryI]", " ", "t"}]}], "]"}], 
        RowBox[{"Zeta", "[", 
         RowBox[{
          RowBox[{"-", 
           FractionBox["3", "2"]}], "-", 
          RowBox[{"\[ImaginaryI]", " ", "t"}]}], "]"}]}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          FractionBox["3", "2"], "+", 
          RowBox[{"\[ImaginaryI]", " ", "t"}]}], ")"}], 
        RowBox[{"Cosh", "[", 
         RowBox[{"\[Pi]", " ", "t"}], "]"}]}]]}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", 
       RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}], ",", 
     RowBox[{"WorkingPrecision", "\[Rule]", "25"}], ",", 
     RowBox[{"PrecisionGoal", "\[Rule]", "20"}], ",", 
     RowBox[{"AccuracyGoal", "\[Rule]", "20"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
