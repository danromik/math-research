Cell[TextData[StyleBox["This section verifies equation (33) in the paper used \
in the proof of Lemma 2.2. First, run the code below to define the functions \
F[n,k], R[n,k] and G[n,k], and the expressions on the two sides of equation \
(33), denoted LHS33 and RHS33. The code also defines a useful function called \
SimplifyTogether[] that brings calculation results to a pleasant form.",
 FontSize->16]], "Text",
 CellChangeTimes->{{3.6350081613100557`*^9, 3.635008231673883*^9}, {
  3.635008266074132*^9, 3.635008266489154*^9}, {3.635018426178177*^9, 
  3.63501842744981*^9}, {3.635030839432863*^9, 3.635030865689671*^9}, {
  3.6350311371890306`*^9, 3.63503113882572*^9}, {3.6843757694902687`*^9, 
  3.684375790883404*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"SimplifyTogether", "[", "expr_", "]"}], ":=", 
    RowBox[{"Together", "[", 
     RowBox[{"FullSimplify", "[", "expr", "]"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"F", "[", 
     RowBox[{"n_", ",", "k_"}], "]"}], ":=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{
       RowBox[{"3", "n"}], "+", "1"}]], "\[Times]", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "n"}], "+", "1"}], ")"}], "!"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"n", "!"}], ")"}], "^", "2"}]], "\[Times]", 
     FractionBox[
      RowBox[{"Binomial", "[", 
       RowBox[{"n", ",", "k"}], "]"}], 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{"3", "n"}], ",", 
        RowBox[{"n", "+", "k"}]}], "]"}]]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"R", "[", 
     RowBox[{"n_", ",", "k_"}], "]"}], ":=", 
    FractionBox[
     RowBox[{"k", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "-", "k", "+", "1"}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"11", 
         SuperscriptBox["n", "2"]}], "+", 
        RowBox[{"2", 
         SuperscriptBox["k", "2"]}], "-", 
        RowBox[{"5", "n", " ", "k"}], "+", 
        RowBox[{"27", "n"}], "-", 
        RowBox[{"6", "k"}], "+", "16"}], ")"}]}], 
     RowBox[{"3", 
      RowBox[{"(", 
       RowBox[{"n", "+", "1"}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"3", "n"}], "+", "2"}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"3", "n"}], "+", "4"}], ")"}], 
      RowBox[{"(", 
       RowBox[{"n", "-", "k", "+", "1"}], ")"}]}]]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"G", "[", 
     RowBox[{"n_", ",", "k_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"F", "[", 
      RowBox[{"n", ",", "k"}], "]"}], 
     RowBox[{"R", "[", 
      RowBox[{"n", ",", "k"}], "]"}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LHS33", "=", 
   RowBox[{
    RowBox[{"F", "[", 
     RowBox[{
      RowBox[{"n", "+", "1"}], ",", "k"}], "]"}], "-", 
    RowBox[{"F", "[", 
     RowBox[{"n", ",", "k"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"RHS33", "=", 
   RowBox[{
    RowBox[{"G", "[", 
     RowBox[{"n", ",", "k"}], "]"}], "-", 
    RowBox[{"G", "[", 
     RowBox[{"n", ",", 
      RowBox[{"k", "+", "1"}]}], "]"}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.634962146830052*^9, 3.634962205106699*^9}, {
  3.634962269073728*^9, 3.6349622843468323`*^9}, {3.634962425133603*^9, 
  3.634962434748472*^9}, {3.635018200716386*^9, 3.635018210714851*^9}, {
  3.635030653057892*^9, 3.6350306892453127`*^9}, {3.684375793066819*^9, 
  3.6843757946893806`*^9}}],

Cell[TextData[StyleBox["The next line will show what LHS33/F[n,k] simplifies \
to after applying the SimplifyTogether function. Run it to see what happens.",
 
 FontSize->16]], "Text",
 CellChangeTimes->{{3.635008241195105*^9, 3.635008263928914*^9}, {
  3.6350308776041594`*^9, 3.635030909476807*^9}, {3.6843757986047287`*^9, 
  3.684375798966894*^9}}],

Cell[BoxData[
 RowBox[{"SimplifyTogether", "[", 
  RowBox[{"LHS33", "/", 
   RowBox[{"F", "[", 
    RowBox[{"n", ",", "k"}], "]"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.634965097510174*^9, 3.6349651015938168`*^9}, {
  3.6350296014251137`*^9, 3.635029602085887*^9}, {3.635029722685851*^9, 
  3.635029770209816*^9}, {3.6350305601550083`*^9, 3.63503056175488*^9}, {
  3.6350306676419973`*^9, 3.635030697766848*^9}, {3.6843758015479794`*^9, 
  3.684375802124858*^9}}],

Cell[TextData[StyleBox["Now do the same for RHS33/F[n,k].",
 FontSize->16]], "Text",
 CellChangeTimes->{{3.635008276904297*^9, 3.6350082862558203`*^9}, 
   3.635030890627729*^9, {3.684375805730522*^9, 3.6843758060744057`*^9}}],

Cell[BoxData[
 RowBox[{"SimplifyTogether", "[", 
  RowBox[{"RHS33", "/", 
   RowBox[{"F", "[", 
    RowBox[{"n", ",", "k"}], "]"}]}], "]"}]], "Input",
 CellChangeTimes->{
  3.634965107593742*^9, {3.635029607273265*^9, 3.635029608523341*^9}, {
   3.635029731372542*^9, 3.635029774484707*^9}, {3.6350305667682343`*^9, 
   3.6350305693423557`*^9}, {3.635030703392672*^9, 3.6350307046289873`*^9}, {
   3.68437580950665*^9, 3.684375810372472*^9}}],

Cell[TextData[StyleBox["You can probably see that the two outputs are the \
same, but just to make sure, run SimplifyTogether on the difference of LHS33 \
and RHS33 divided by F[n,k]. If you get 0, that means equation (33) is \
correct.",
 FontSize->16]], "Text",
 CellChangeTimes->{{3.635008295465539*^9, 3.6350083249582777`*^9}, {
  3.635030898464595*^9, 3.635030917723467*^9}, {3.684375813555394*^9, 
  3.6843758203988523`*^9}}],

Cell[BoxData[
 RowBox[{"SimplifyTogether", "[", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"LHS33", "-", "RHS33"}], ")"}], "/", 
   RowBox[{"F", "[", 
    RowBox[{"n", ",", "k"}], "]"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.63496236442066*^9, 3.634962378233049*^9}, {
  3.6349624165955276`*^9, 3.634962418545107*^9}, {3.635029616436335*^9, 
  3.635029617323111*^9}, {3.635029784760439*^9, 3.6350297870847273`*^9}, {
  3.63503070810518*^9, 3.635030709991713*^9}, {3.684375823715889*^9, 
  3.6843758254967203`*^9}}]
}, Closed]],

Cell[CellGroupData[{

Cell["Verification of Lemma 2.3", "Subsection",
 CellChangeTimes->{{3.6349625387837143`*^9, 3.63496254125778*^9}}],

Cell[TextData[StyleBox["Here, we verify equation (38) used in the proof of \
Lemma 2.3. First, run the code below to load the definitions of the functions \
F1, F2, U, V, W, R1, R2, G1, G2.",
 FontSize->16]], "Text",
 CellChangeTimes->{{3.635008336543633*^9, 3.63500837335717*^9}, {
  3.6843758745295057`*^9, 3.684375874652548*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"SimplifyTogether", "[", "expr_", "]"}], ":=", 
    RowBox[{"Together", "[", 
     RowBox[{"FullSimplify", "[", "expr", "]"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Repeat", " ", "the", " ", "definition", " ", "of", " ", 
    "SimplifyTogether", " ", "from", " ", "the", " ", "previous", " ", 
    "section", " ", "so", " ", "both", " ", "sections", " ", "can", " ", "be",
     " ", "run", " ", "independently"}], " ", "*)"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"F1", "[", 
    RowBox[{"n_", ",", "m_", ",", "j_"}], "]"}], ":=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}], 
     RowBox[{"n", "+", "j"}]], 
    FractionBox[
     RowBox[{
      RowBox[{"Binomial", "[", 
       RowBox[{"n", ",", "j"}], "]"}], 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{"3", "n"}], ",", 
        RowBox[{"m", "+", "j", "-", "n"}]}], "]"}]}], 
     RowBox[{"Binomial", "[", 
      RowBox[{
       RowBox[{"3", "n"}], ",", 
       RowBox[{"n", "+", "j"}]}], "]"}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"F2", "[", 
     RowBox[{"n_", ",", "m_", ",", "j_"}], "]"}], ":=", 
    FractionBox[
     RowBox[{
      RowBox[{"Binomial", "[", 
       RowBox[{"n", ",", "j"}], "]"}], 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{"n", "+", "j", "-", "1"}], ",", 
        RowBox[{"m", "+", "j", "-", "n"}]}], "]"}]}], 
     RowBox[{"Binomial", "[", 
      RowBox[{
       RowBox[{"3", "n"}], ",", 
       RowBox[{"n", "+", "j"}]}], "]"}]]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"U", "[", 
    RowBox[{"n_", ",", "m_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"m", "+", "2"}], ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", "n"}], "-", "m", "-", "1"}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"V", "[", 
    RowBox[{"n_", ",", "m_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "2"}], 
     SuperscriptBox["m", "2"]}], "-", 
    RowBox[{"5", 
     SuperscriptBox["n", "2"]}], "+", 
    RowBox[{"8", "m", " ", "n"}], "+", 
    RowBox[{"9", "n"}], "-", 
    RowBox[{"4", "m"}], "-", "2"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"W", "[", 
     RowBox[{"n_", ",", "m_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"4", "n"}], "-", "m"}], ")"}]}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "n"}], "-", "m", "-", "1"}], ")"}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"R1", "[", 
    RowBox[{"n_", ",", "m_", ",", "j_"}], "]"}], ":=", 
   FractionBox[
    RowBox[{"j", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"3", "n"}], "+", "1"}], ")"}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "n"}], "-", "j", "+", "1"}], ")"}]}], 
    RowBox[{"n", "-", "m", "-", "j", "-", "1"}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"R2", "[", 
     RowBox[{"n_", ",", "m_", ",", "j_"}], "]"}], ":=", 
    FractionBox[
     RowBox[{"j", 
      RowBox[{"(", 
       RowBox[{"m", "-", 
        RowBox[{"2", "n"}], "+", "1"}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "n"}], "-", "j", "+", "1"}], ")"}]}], 
     RowBox[{"n", "-", "m", "-", "j", "-", "1"}]]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"G1", "[", 
    RowBox[{"n_", ",", "m_", ",", "j_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"F1", "[", 
     RowBox[{"n", ",", "m", ",", "j"}], "]"}], 
    RowBox[{"R1", "[", 
     RowBox[{"n", ",", "m", ",", "j"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"G2", "[", 
    RowBox[{"n_", ",", "m_", ",", "j_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"F2", "[", 
     RowBox[{"n", ",", "m", ",", "j"}], "]"}], 
    RowBox[{"R2", "[", 
     RowBox[{"n", ",", "m", ",", "j"}], "]"}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.634962596715592*^9, 3.634962641227654*^9}, {
  3.634962802545331*^9, 3.63496284505541*^9}, {3.6350182406239567`*^9, 
  3.6350183005639353`*^9}, {3.635030929995367*^9, 3.635030972307712*^9}}],

Cell[TextData[StyleBox["Now, define the expressions on the left-hand side and \
the right-hand side of equation (38). Note that there is a parameter \[Alpha] \
that takes the values \[Alpha]=1 and \[Alpha]=2, so we define two quantities, \
LHS38one and LHS38two, for the left-hand side (corresponding to the two \
values of \[Alpha]), and similarly for the right-hand side.",
 FontSize->16]], "Text",
 CellChangeTimes->{{3.635008379260356*^9, 3.635008433818519*^9}, {
  3.684375886449155*^9, 3.684375891753776*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"LHS38one", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"U", "[", 
      RowBox[{"n", ",", "m"}], "]"}], 
     RowBox[{"F1", "[", 
      RowBox[{"n", ",", 
       RowBox[{"m", "+", "2"}], ",", "j"}], "]"}]}], "+", 
    RowBox[{
     RowBox[{"V", "[", 
      RowBox[{"n", ",", "m"}], "]"}], 
     RowBox[{"F1", "[", 
      RowBox[{"n", ",", 
       RowBox[{"m", "+", "1"}], ",", "j"}], "]"}]}], "+", 
    RowBox[{
     RowBox[{"W", "[", 
      RowBox[{"n", ",", "m"}], "]"}], 
     RowBox[{"F1", "[", 
      RowBox[{"n", ",", "m", ",", "j"}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"RHS38one", "=", 
    RowBox[{
     RowBox[{"G1", "[", 
      RowBox[{"n", ",", "m", ",", 
       RowBox[{"j", "+", "1"}]}], "]"}], "-", 
     RowBox[{"G1", "[", 
      RowBox[{"n", ",", "m", ",", "j"}], "]"}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"LHS38two", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"U", "[", 
      RowBox[{"n", ",", "m"}], "]"}], 
     RowBox[{"F2", "[", 
      RowBox[{"n", ",", 
       RowBox[{"m", "+", "2"}], ",", "j"}], "]"}]}], "+", 
    RowBox[{
     RowBox[{"V", "[", 
      RowBox[{"n", ",", "m"}], "]"}], 
     RowBox[{"F2", "[", 
      RowBox[{"n", ",", 
       RowBox[{"m", "+", "1"}], ",", "j"}], "]"}]}], "+", 
    RowBox[{
     RowBox[{"W", "[", 
      RowBox[{"n", ",", "m"}], "]"}], 
     RowBox[{"F2", "[", 
